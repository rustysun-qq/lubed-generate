{
    "name": "lubed/generate",
    "description": "Yet another php7+ code generator!",
    "license": "Apache-2.0",
    "version": "0.1.0",
    "authors": [
        {
            "name": "cjj",
            "email": "cjj8110@163.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "vlucas/phpdotenv": "5.4.*",
        "symfony/console": "5.4.*",
        "lubed/utils": "0.1",
        "lubed/data": "0.1"
    },
    "require-dev": {
        "symfony/var-dumper": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Lubed\\": "src/"
        }
    },
    "bin": ["lubed"],
    "config": {
        "bin-dir": "scripts"
    }
}
